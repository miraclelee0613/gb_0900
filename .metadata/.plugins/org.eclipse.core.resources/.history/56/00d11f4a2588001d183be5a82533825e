//		result = "" + (num > 0 ? (num > (int)num ? num : (int)num) : (num < (int)num ? num : (int)num));
		result = num > 0 ? (num > (int)num ? "%.0f" : "%.0f") : (num < (int)num ? "%.0f" : "%.0f");
		System.out.printf(result, num > 0 ? (num > (int)num ? num : (int)num) : (num < (int)num ? num : (int)num));
		
		
		
		
		
		
		
		
		
		
		
		
		
		
